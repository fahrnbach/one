{
  "name": "omnibar",
  "projectType": "application",
  "sourceRoot": "apps/core/api/omnibar",
  "targets": {
    "build": {
      "executor": "@nx/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/core/api/omnibar",
        "main": "apps/core/api/omnibar/src/main.ts",
        "tsConfig": "apps/core/api/omnibar/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "@nx/node:execute",
      "options": {
        "buildTarget": "components:omnibar:build"
      }
    }
  },
  "tags": ["type:ui", "scope:components"]
}
